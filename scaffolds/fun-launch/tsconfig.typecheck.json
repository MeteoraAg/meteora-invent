{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "allowUnreachableCode": true
  },
  "include": ["next-env.d.ts", "src/**/*", "src/**/*.tsx", "src/**/*.ts"]
}
